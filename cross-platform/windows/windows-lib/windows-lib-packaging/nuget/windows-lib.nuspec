<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>windows-lib</id>
    <version>1.0.0</version>
    <authors>Yigit Cagri Akkaya</authors>
    <description>NuGet package to access common library</description>
    <dependencies>
      <!--Note: Target .NET 5 by replacing instances of "net6.0" with "net5.0 in this file"-->
      <group targetFramework="net5.0-windows10.0.19041.0" />
    </dependencies>
  </metadata>
  <files>
    <!--Note: This sample assumes the configuration/platform are set to Release/x64-->
    <!--Projection assembly is architecture-neutral-->
    <file src="..\..\build\x64\Release\windows-lib\bin\windows-lib\windows-lib.dll" target="lib\net5.0-windows10.0.19041.0\WindowsImpactLibProjection.dll" />
    <file src="..\..\..\sqlite\sqlite3.dll" target="lib\net5.0-windows10.0.19041.0\sqlite3.dll"/>
    <!--Arch-specific implementation DLLs should be copied into RID-relative folders -->
    <file src="..\..\build\x64\Release\windows-lib\bin\windows-lib\windows-lib.dll" target="runtimes\win10-x64\native\windows-lib.dll" />
    <file src="..\..\..\sqlite\sqlite3.dll" target="runtimes\win10-x64\native\sqlite3.dll"/>
  </files>
</package>